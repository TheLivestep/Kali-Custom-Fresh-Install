grep -o '[^ ]*[A-Z][^ ]*' > contains_uppercase.txt 	# grep only uppercases
grep -o '[^ ]*[0-9][^ ]*' > contains_numbers.txt 	# grep only numbers
grep -v "^[A-Za-z0-9]*$" > contains_special.txt 	# grep only special characters
